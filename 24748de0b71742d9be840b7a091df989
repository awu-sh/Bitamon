<script>
  let timeId = setInterval(() => {
    const priority = document.querySelector("#priority");
    if (!priority) {
      clearInterval(timeId);
      return;
    }
    const data = priority.getAttribute("data-animateType");
    if (!priority) {
      clearInterval(timeId);
      return;
    }
    if (priority.className) {
      priority.className = "";
      return;
    }
    priority.className = `animate__animated  animate__${data} buttonBox`;
  }, 1000);

  const videoAll = [...document.querySelectorAll(".video")];
  videoAll.forEach((item) => {
    let height = (item.clientWidth * 1080) / 1920;
    item.style.height = height + "px";
  });
  const audio = [...document.querySelectorAll(".audio")];
  audio.forEach((item) => {
    let height = (item.clientWidth * 251) / 675;
    item.style.height = height + "px";
  });
</script>


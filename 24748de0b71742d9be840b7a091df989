<script>
  let footer = document.getElementsByClassName("footer");
  footer = [...footer];
  footer.forEach((item) => {
    item.onclick = function () {
      window.open("http://www.desty.app");
    };
  });
  const whiteboard = document.getElementById("whiteboard");
  const fullScreen = document.querySelector(".fullScreen");
  const background = JSON.parse(whiteboard.getAttribute("background"));
  function setColor(color) {
    fullScreen.style.backgroundColor = color;
  }
  switch (background.type) {
    case "freeimage":
      if (background.setting.imgSrc) {
        fullScreen.style.backgroundImage = `url(${background.setting.imgSrc})`;
      } else {
        setColor(background.setting.value);
      }
      break;
    case "image":
      if (background.setting.imgSrc) {
        fullScreen.style.backgroundImage = `url(${background.setting.imgSrc})`;
      } else {
        setColor(background.setting.value);
      }
      break;
    case "color":
      setColor(background.setting.color);
      break;
    default:
      break;
  }
  const footerstyle = JSON.parse(whiteboard.getAttribute("footerstyle"));
  const bgColor = footerstyle.bgColor;
  const fontColor = footerstyle.fontColor;
  const fontFamily = footerstyle["font-family"];
  const svgStyle = footerstyle.svgStyle;
